@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="col-md-12">
        <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <h1 class="text-center">Welcome to Sympo</h1>
            <h2 class="text-center">We are here to help you organize your next Symposium!</h2>
    <p> Whether you are meeting for business or having a family get together you can use this application to reserve your next gathering location, inform the front desk or administration of your future meeting as well as alert any gated security of the incoming guests or clients!
        All at the ease of this simple application...it's SYMPO!!</p>
        <hr>
        <p>First time users please use the button on the top right hand side of your page to register for your account!</p>        
        
        <p>Already registered?  Go ahead and hit the login button above to reserve your next room!</p>
        
        </div>
        </div>
    </div>
</div>
<div id="carouselExampleIndicators" class="carousel slide carousel-fade" data-ride="carousel">
  <div class="carousel-inner" role="listbox">
    <div class="carousel-item active">
      <img class="d-block w-100" src="https://i.pinimg.com/originals/ba/0c/8f/ba0c8f5a4e7a5d6ad17d830ed1c6ba72.jpg" data-src="conferenceRoom.jpg" img id= "conferenceView" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" style="wdith:900px;height:400px;" src="https://images.meetingsbooker.com/images/venues/londonmarriotthotelcanarywharf-0.jpg" data-src="bellatory room.jpg" img id="bellatoryRoom" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" style="wdith:900px;height:400px;" src="https://i.pinimg.com/originals/77/de/76/77de764e981c0efba28de6c20c242523.jpg" data-src="lakeview room.jpg" alt="Third slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" style="width:900px;height:400px;" src="https://assets.hyatt.com/content/dam/hyatt/hyattdam/images/2015/03/30/1631/Hyatt-Ziva-Rose-Hall-P065-St-Andrews.jpg/Hyatt-Ziva-Rose-Hall-P065-St-Andrews.16x9.jpg?imwidth=1280" data-src="rose room" alt="Fourth slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" style="wdith:600px;height:400px;" src="https://www.ribbyhall.co.uk/uploads/images/header-spa/zen-garden-header.jpg" data-src="zen garden.jpg" alt="Fifth slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" style="wdith:900px;height:400px;" src="https://i.pinimg.com/originals/d4/a0/fe/d4a0fe1a15792062c73da80d60ca6aff.png" data-src="waves conference room.png" alt="sixth slide">
    </div>    
  </div>
  <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
